# --------------
# Rain inference
# --------------
#
# Infer that the grass becomes wet.

@prefix lingua: <http://www.w3.org/2000/10/swap/lingua#>.
@prefix : <#>.

:currentWeather a :NormalWeather.
:obs1 :ascribed :Rain.

() lingua:onNegativeSurface _:ns1.

_:ns1 {
    :currentWeather a :NormalWeather.
    () lingua:onNegativeSurface _:ns2.
}

_:ns2 {
    (_:S) lingua:onNegativeSurface _:ns3.
}

_:ns3 {
    _:S :ascribed :Rain.
    () lingua:onNegativeSurface _:ns4.
}

_:ns4 {
    _:S :ascribed :WetGrass.
}

# query
(_:S _:O) lingua:onQuestionSurface _:qs.

_:qs {
    _:S :ascribed _:O.
}

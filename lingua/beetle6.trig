@prefix lingua: <http://www.w3.org/2000/10/swap/lingua#>.
@prefix : <#>.

# beetle is a car
:beetle a :Car.

# all cars are green or blue
(_:A) lingua:nand _:ng1.

_:ng1 {
    _:A a :Car.
    () lingua:nand _:ng2.
    () lingua:nand _:ng3.
}

_:ng2 {
    _:A :is :green.
}

_:ng3 {
    _:A :is :blue.
}

# green things are nice or pretty
(_:A) lingua:nand _:ng4.

_:ng4 {
    _:A :is :green.
    () lingua:nand _:ng5.
    () lingua:nand _:ng6.
}

_:ng5 {
    _:A :is :nice.
}

_:ng6 {
    _:A :is :pretty.
}

# pretty things are beautiful
(_:A) lingua:nand _:ng7.

_:ng7 {
    _:A :is :pretty.
    () lingua:nand _:ng8.
}

_:ng8 {
    _:A :is :beautiful.
}

# cars are not beautiful
(_:A) lingua:nand _:ng9.

_:ng9 {
    _:A a :Car.
    _:A :is :beautiful.
}

# blue things are beautiful
(_:A) lingua:nand _:ng10.

_:ng10 {
    _:A :is :blue.
    () lingua:nand _:ng11.
}

_:ng11 {
    _:A :is :beautiful.
}

# query
(_:S) lingua:question _:qs.

_:qs {
    _:S :is :nice.
}
